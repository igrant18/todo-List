{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "allowJs": false,
        "checkJs": false,
        "jsx": "react",
        "outDir": "./build",
        "removeComments": true,
        "noEmit": true,
        "pretty": true,
        "skipLibCheck": true,
        "strict": false,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "downlevelIteration": true
    },
    "include": [
        "./src/"
    ],
    "exclude": [
        "node_modules"
    ]
}